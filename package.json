{
  "name": "momumental-duo-app",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "shakecss": "purgecss --css dist/browser/*.css --content dist/**/*.html dist/**/*.js --out dist/browser/",
    "prebuild": "npm run clean:build",
    "buildshake": "npm-run-all build:client webpack:server shakecss",
    "build:client": "ng build --prod",
    "clean:build": "rimraf dist build unnamed.tar.gz",
    "rimraf": "rimraf",
    "prettify": "npm-run-all prettify:base:ts",
    "prettify:base:ts": "prettier --single-quote --write --trailing-comma=all \"./!(.git|dist|build)/**/*.ts\"",
    "startserver": "./node_modules/cross-env/dist/bin/cross-env.js NODE_ENV=production node dist/server.js --config $APP_CONFIG_LOCATION",
    "start:local": "node --max_old_space_size=4096 dist/server.js --config tools/salt-config/iris.properties",
    "testjest": "npm run lint && jest",
    "test:watch": "jest --watch",
    "test:ci": "node --max_old_space_size=4096 ./node_modules/jest-cli/bin/jest --maxWorkers=5",
    "webpack:server": "webpack --config webpack.server.config.js --progress --colors"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~8.2.4",
    "@angular/cdk": "~8.2.0",
    "@angular/common": "~8.2.4",
    "@angular/compiler": "~8.2.4",
    "@angular/core": "~8.2.4",
    "@angular/forms": "~8.2.4",
    "@angular/material": "^8.2.0",
    "@angular/platform-browser": "~8.2.4",
    "@angular/platform-browser-dynamic": "~8.2.4",
    "@angular/router": "~8.2.4",
    "hammerjs": "^2.0.8",
    "rxjs": "~6.4.0",
    "tslib": "^1.10.0",
    "zone.js": "~0.9.1"
    },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.803.3",
    "@angular/cli": "~8.3.3",
    "@angular/compiler-cli": "~8.2.4",
    "@angular/language-service": "~8.2.4",
    "@types/node": "~8.9.4",
    "@types/jasmine": "~3.3.8",
    "@types/jasminewd2": "~2.0.3",
    "codelyzer": "^5.0.0",
    "jasmine-core": "~3.4.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~4.1.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~2.0.1",
    "karma-jasmine-html-reporter": "^1.4.0",
    "protractor": "~5.4.0",
    "ts-node": "~7.0.0",
    "tslint": "~5.15.0",
    "typescript": "~3.5.3",
    "@types/jest": "^23.3.1",
    "husky": "^1.3.1",
    "jest": "22.4.1",
    "jest-cli": "^23.6.0",
    "jest-junit": "^5.1.0",
    "jest-message-util": "^23.4.0",
    "jest-preset-angular": "^6.0.0",
    "jest-zone-patch": "^0.0.8",
    "npm-run-all": "^4.1.3",
    "prettier": "~1.13.0",
    "pretty-quick": "^1.10.0",
    "purgecss": "^1.1.0",
    "redux-devtools-extension": "^2.13.8",
    "ts-jest": "^23.1.2",
    "ts-loader": "^4.4.1",
    "tsickle": "^0.30.0",
    "tslib": "^1.7.1",
    "tslint-loader": "~3.6.0",
    "webpack-cli": "^3.2.3",
    "webpack-node-externals": "^1.7.2"
  }
}
